{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "UseSecretManager": false,
  "JwtOptions": {
    "Lifetime": 30,
    "SecretKey": "SET_THE_SECRET_KEY_HERE",
    "ValidAudience": "storeApp",
    "ValidIssuer": "identity",
    "ValidateAudience": true,
    "ValidateIssuer": true,
    "ValidateLifetime": true
  },
  "CatalogDatabaseConnectionString": {
    "ConnectionString": "Host=localhost;Port=5432;Database=StoreCatalog;Username=__USERNAME__;Password=**********;Include Error Detail=true"
  },
  "RabbitMqSettings": {
    "Host": "localhost",
    "UserName": "*********",
    "Password": "***********",
    "VirtualHost": "/"
  },
  "ServiceRegistration": {
    "ServiceDiscoveryAddress": "http://localhost:8500",
    "Host": "https://localhost",
    "Port": 7291,
    "ServiceId": "catalog"
  }
}
